<div class="status-container">
  <div class="status">

    {{#ifeq type "success" }}
    <img src="/images/thank-you.svg" class="status-image" alt="">
    {{/ifeq}}
    {{#ifeq type "error" }}
    <img src="/images/error.svg" class="status-image" alt="">
    {{/ifeq}}

    <p class="status-message">
      {{#ifeq type "success" }}
      Disabled RecurringDetailReference {{recurringDetailReference}}
      {{/ifeq}}
      {{#ifeq type "error" }}
      Could not disable RecurringDetailReference '{{recurringDetailReference}}'. See logs for more information about the response.</a>
      {{/ifeq}}
    </p>
    <a class="button" href="/admin" to="#/admin">Return</a>

  </div>

</div>